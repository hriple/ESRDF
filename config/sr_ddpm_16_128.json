{
    "name": "sr_ffhq_16_128",
    "phase": "train",
    "gpu_ids": [
        0
    ],
    "path": {
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": null
    },
    "datasets": {
        "train": {
            "name": "FFHQ",
            "mode": "HR",
            "dataroot": "/newHome/S2_HHH/gitte/ddpm_our/dataset/train_FFHQ_32_128",
            "datatype": "img", //lmdb or img, path of img files
            "l_resolution": 16,
            "r_resolution": 128,
			// "batch_size": 8,
            "batch_size": 80,
            "num_workers": 3,
            "use_shuffle": true,
            "data_len": -1
        },
        "val": {
            "name": "FFHQ",
            "mode": "LRHR",
            "dataroot": "/newHome/S2_HHH/gitte/ddpm_our/dataset/test_FFHQ_32_128",
            "datatype": "img", //lmdb or img, path of img files
            "l_resolution": 16,
            "r_resolution": 128,
			"batch_size": 60,
			"num_workers": 3,
		    // 是否乱序
		    "use_shuffle": true,
            "data_len": -1
        }
    },
    "model": {
        "which_model_G": "ddpm", //ddpm, sr3
        "finetune_norm": false,
        "unet": {
            "in_channel": 6,
            "out_channel": 3,
            "inner_channel": 64,
			// 1 2 4 8 8
            "channel_multiplier": [1, 2, 4, 8, 8],
            "attn_res": [
               16
            ],
            "res_blocks": 2,
			// 0.2
            "dropout": 0.0
        },
        "beta_schedule": {
            "train": {
				//  linear
                "schedule": "linear", 
                // "n_timestep": 2000,
				"n_timestep": 500,
				// 1e-4
                "linear_start": 1e-6,
				// 2e-2
                "linear_end": 1e-2
            },
            "val": {
                "schedule": "linear",
                // "n_timestep": 2000,
				"n_timestep": 500,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 128,
            "channels": 3, //sample channel
            "conditional": true
        }
    },
    "train": {
        "n_iter": 800000,
        "val_freq": 1e4,
        "save_checkpoint_freq": 1e5,
		// "print_freq": 600, batch_size = 8 是 1 epoch,
        "print_freq": 1800,
        "optimizer": {
            "type": "adamw",
            "lr": 1e-4
        },
        "ema_scheduler": {
            "step_start_ema": 5000,
            "update_ema_every": 1,
            "ema_decay": 0.9999
        }
    },
    "wandb": {
        "project": "sr_ffhq_16_128"
    }
}