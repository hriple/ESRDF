{
	"name": "FFHQ",
	"phase": "train",
	// train or val
	"gpu_ids": [
	  0
	],
	"path": {
	  //set the path
	  "log": "logs",
	  "tb_logger": "tb_logger",
	  "results": "results",
	  "checkpoint": "checkpoint",
  //    "resume_state": "predicted_model/sr_FFHQ_241028_004900/I80000_E160"
  //    "resume_state": "experiments/sr_FFHQ_241028_185325/checkpoint/I100_E1"
	  "resume_state": null
	  //pretrain model or training state
	},
	"datasets": {
	  "train": {
		"name": "FFHQ_train_4800_16_128",
		"mode": "HR",
		// whether need LR img
		"dataroot": "/newHome/S2_HHH/SR3_res/dataset/FFHQ_train_4800_16_128",
		"datatype": "img",
		//lmdb or img, path of img files
		"l_resolution": 32,
		// low resolution need to super_resolution
		"r_resolution": 128,
		// high resolution
  //      "batch_size": 4,
		"batch_size": 8,
		"num_workers": 8,
		// 是否乱序
		"use_shuffle": true,
		"data_len": -1
		// -1 represents all data used in train
	  },
	  "val": {
		"name": "FFHQ_test_1200_16_128",
		"mode": "LRHR",
		"dataroot": "/newHome/S2_HHH/SR3_res/dataset/FFHQ_test_1200_16_128",
		"datatype": "img",
		//lmdb or img, path of img files
		"l_resolution": 32,
		"r_resolution": 128,
		"batch_size": 8,
		"num_workers": 8,
		// 是否乱序
		"use_shuffle": true,
		// 此处为自己修改，原本是没有 这个batch_size的。
		"data_len": -1
		// -1 就是 拿所有数据集
		// data length in validation
	  },
	  "mface":{
		  "dataroot": "/newHome/S2_HHH/SR3_res/dataset/FFHQ_train_4800_16_128"
	  }
	},
	"model": {
	  "which_model_G": "sr3",
	  // use the ddpm or sr3 network structure
	  "finetune_norm": false,
	  "unet": {
		"in_channel": 6,
		"out_channel": 3,
		"inner_channel": 64,
		"channel_multiplier": [
		  1,
		  2,
		  4,
		  8,
		  8
		],
		"attn_res": [
		  16
		],
		"res_blocks": 2,
		"dropout": 0.0
	  },
	  "beta_schedule": {
		// use munual beta_schedule for acceleration
		"train": {
		  "schedule": "linear",
		//  "n_timestep": 2000,
		"n_timestep": 2000,
		  "linear_start": 1e-6,
		  "linear_end": 1e-2
		},
		"val": {
		  "schedule": "linear",
		//  "n_timestep": 2000,
		"n_timestep": 2000,
		  "linear_start": 1e-6,
		  "linear_end": 1e-2
		}
	  },
	  "diffusion": {
		"image_size": 128,
		"channels": 3,
		//sample channel
  //      "conditional": true
		"conditional": true
		// unconditional generation or unconditional generation(super_resolution)
	  }
	},
	"train": {
	  "n_iter": 100000,
	  "val_freq": 1e4,
	  "save_checkpoint_freq": 1e4,
  //    200 iter 输出一次
	  "print_freq": 200,
	  "optimizer": {
		"type": "adam",
		"lr": 1e-4
	  },
	  "ema_scheduler": {
		// not used now
		"step_start_ema": 5000,
		"update_ema_every": 1,
		"ema_decay": 0.9999
	  }
	},
	"wandb": {
	  "project": "FFHQ"
	}
  }